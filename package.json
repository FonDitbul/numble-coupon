{
  "name": "numble-coupon",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "dotenv -e .env.production -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "dotenv -e .env -- npx prisma migrate dev --name postgres-init",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate dev --name postgres-init",
    "migrate:docker": "dotenv -e .env.production.docker -- npx prisma migrate dev --name postgres-init",
    "push:dev": "dotenv -e .env -- npx prisma db push dev",
    "push:prod": "dotenv -e .env.production -- npx prisma db push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:migrate": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
    "test:e2e": "dotenv -e .env.test -- jest --config test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.6",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@prisma/client": "^4.13.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^3.0.1",
    "date-fns": "^2.0.0-beta.5",
    "dotenv": "^16.0.3",
    "ioredis": "^5.3.2",
    "jest-mock-extended": "^3.0.4",
    "prisma": "^4.13.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "webpack": "^5.80.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.2.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^9.4.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "supertest": "^6.3.3",
    "testcontainers": "^9.6.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
